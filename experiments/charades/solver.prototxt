train_net: "./experiments/charades/train.prototxt"
base_lr: 0.0001
#base_lr: 0.00001
lr_policy: "step"
gamma: 0.1
stepsize: 350000
display: 1
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own
# snapshot function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "./experiments/charades/snapshot/charades"
iter_size: 1
